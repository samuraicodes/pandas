import pandas

dataframe = pandas.read_excel('pandas.xlsx', 'Tabellenblatt1') #can be writed like df

print(dataframe) #this method give us the type from dataframe -> <class 'pandas.core.frame.DataFrame'>

dataframe_cvs = pandas.read_csv('pandas.csv')

print(dataframe_cvs)

dataframe_txt = pandas.read_csv('pandas.txt') #With the cvs read method we can read txt files too.

print(dataframe_txt)

dataframe_txt_tab = pandas.read_csv('pandas2.txt', delimiter='/') #if we use delimiter we can change , for other symbol

print(dataframe_txt_tab)